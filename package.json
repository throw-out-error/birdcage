{
    "name": "@toes/birdcage",
    "version": "0.0.1",
    "description": "",
    "license": "GNU GPL V3",
    "files": [
        "dist/",
        "src/"
    ],
    "bin": {
        "birdcage": "dist/backend/cli.js"
    },
    "scripts": {
        "dev": "run-p npm run dev-backend dev-frontend",
        "build": "run-p build-backend build-frontend",
        "build-clean": "run-s clear && build",
        "start": "run-s build server",
        "server": "node dist/backend/main.js",
        "build-backend": "cross-env NODE_ENV=production tsc --project tsconfig.backend.json",
        "dev-backend": "ts-node-dev src/backend/main.ts",
        "build-frontend": "cross-env NODE_ENV=production parcel build src/frontend/*.html --public-url /panel --out-dir dist/www",
        "dev-frontend": "parcel watch src/frontend/*.html --public-url /panel --out-dir dist/www",
        "clear": "rimraf .cache & rimraf dist"
    },
    "ultra": {
        "concurrent": [
            "dev"
        ]
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.1",
        "@babel/plugin-transform-react-jsx": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@types/bcrypt": "3.0.0",
        "@types/connect-redis": "0.0.14",
        "@types/express": "4.17.8",
        "@types/express-session": "1.17.0",
        "@types/http-proxy": "1.17.4",
        "@types/node": "14.14.6",
        "@types/validator": "^13.1.0",
        "@typescript-eslint/eslint-plugin": "4.6.0",
        "@typescript-eslint/parser": "4.6.0",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "cross-env": "7.0.2",
        "cssnano": "4.1.10",
        "eslint": "7.19.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-prettier": "3.1.4",
        "listr2": "3.2.2",
        "node-sass": "4.14.1",
        "npm-run-all": "^4.1.5",
        "parcel": "^1.12.4",
        "prettier": "2.1.2",
        "rimraf": "3.0.2",
        "sass": "^1.27.1",
        "ts-loader": "8.0.7",
        "ts-node-dev": "1.0.0",
        "typescript": "4.0.5",
        "ultra-runner": "3.5.0"
    },
    "dependencies": {
        "@keyv/sqlite": "^2.0.2",
        "@overnightjs/core": "^1.7.5",
        "@toes/core": "^0.0.6-dev",
        "@types/keyv": "^3.1.1",
        "@types/keyv__sqlite": "^2.0.1",
        "ajv": "^6.12.6",
        "axios": "^0.21.0",
        "bcrypt": "^5.0.0",
        "chalk": "^4.1.0",
        "commander": "^6.2.0",
        "connect-redis": "^5.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "http-proxy": "^1.18.1",
        "keyv": "^4.0.3",
        "knex": "^0.21.8",
        "mime": "^2.4.6",
        "preact": "^10.5.5",
        "preact-router": "^3.2.1",
        "rate-limiter-flexible": "^2.1.13",
        "reflect-metadata": "^0.1.13",
        "restyped": "^1.1.0",
        "restyped-axios": "^2.0.0",
        "typed-path": "^2.0.2",
        "typedi": "^0.8.0",
        "unistore": "^3.5.2",
        "validator": "^13.1.17",
        "yaml": "^1.10.0"
    },
    "alias": {
        "shared": "./src/shared",
        "frontend": "./src/frontend",
        "backend": "./src/backend"
    }
}
